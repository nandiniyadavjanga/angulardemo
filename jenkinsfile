pipeline { 
    agent {
      docker {
        image '
    stages {
        stage ('Checkout') {
            steps {
                sh 'git checkout master'
            }
        }
        stage('Install Packages') {
      steps {
        sh 'npm install'
      }
    }
    stage('Test and Build') {
      parallel {
        stage('Run Tests') {
          steps {
            sh 'npm run test'
          }
        }
        stage('Create Build Artifacts') {
          steps {
            sh 'npm run build'
          }
        }
      }
    }
    }
}
